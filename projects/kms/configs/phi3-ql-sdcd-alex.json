{
    "loss_function": "dcd",
    "dataset_type": "dcd_km",
    "evaluate_on": "quality-rag",
    "use_context": true,
    "topk_context": 5,
    "dataset": "az://mttldata/quality-summaries-qa-phi-3-medium",
    "padding_side": "right",
    "truncation_side": "left",
    "max_input_length": 4096,
    "eval_every_n_epoch": 1,
