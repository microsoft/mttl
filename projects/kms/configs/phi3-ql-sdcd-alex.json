    "trainable_param_names": ".*lora_[ab].*",
    "learning_rate": 1e-3,
    "micro_batch_size": 8,
    "train_batch_size": 8,
    "predict_batch_size": 16,
    "precision": "bf16",
    "model": "microsoft/Phi-3-mini-4k-instruct",
    "model_family": "gpt",
    "optimizer": "adamw",
    "warmup_proportion": 0.06,
    "attn_implementation": "flash_attention_2",
    "task_name_field": "document_id"
