{   
    "num_train_epochs": 1,
    "subsample_dev": 1000,
    "model": "microsoft/phi-2",
    "learning_rate": 1e-4,
    "weight_decay": 0.0,
    "n_skills": 1,
    "micro_batch_size": 1,
    "train_batch_size": 16,
    "predict_batch_size": 4,
    "precision": "bf16",
    "dataset": "sordonia/flan-10k-flat",
    "model_family": "gpt",
    "load_in_8bit": false,
    "optimizer": "adamw",
    "warmup_proportion": 0.06,
    "max_input_length": 2048,
    "max_output_length": 64,
    "truncation_side": "left",
    "eval_before_training": true,
    "eval_rouge_flag": true,
    "pipeline_eval_tasks": false,
    "use_sparse_model": true,
    "model_modifier": "scatter_sparse_mask_adapter",
    "modify_modules": ".*",
    "modify_layers": ".*dense|.*q_proj|.*v_proj|.*k_proj",
    "trainable_param_names": ".*sparse_layer.*",
    "keep_ratio": 0.05,
    "sparse_cat": "regular_sparse",
    "activate_scatter_training": true
    

}


