{
    "lora_rank": 4,
    "lora_dropout": 0.05,
    "weight_decay": 0.0,
    "n_splits": 1, 
    "model_modifier": "lora",
    "modify_modules": ".*",
    "modify_layers": ".*dense|.*q_proj|.*v_proj|.*k_proj",
    "trainable_param_names": ".*lora_[ab].*",
    "num_train_epochs": 1,
    "learning_rate": 1e-4,
    "micro_batch_size": 1,
    "train_batch_size": 16,
    "predict_batch_size": 4,
    "subsample_dev": 1000,
    "precision": "bf16",
    "dataset": "sordonia/flan-10k-flat",
    "model": "microsoft/phi-2",
    "model_family": "gpt",
    "load_in_8bit": false,
    "optimizer": "adamw",
    "warmup_proportion": 0.06,
    "max_input_length": 2048,
    "max_output_length": 64,
    "truncation_side": "left",
    "remove_phi_eval_tasks": true,
    "pipeline_eval_tasks": "all",
    "tie_params": "q_proj.lora_a|k_proj.lora_a|v_proj.lora_a"
}
