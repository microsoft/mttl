
{
    "num_train_epochs": 5,
    "model": "microsoft/Phi-3-mini-4k-instruct",
    "learning_rate": 1e-4,
    "weight_decay": 0.0,
    "n_skills": 1,
    "micro_batch_size": 1,
    "train_batch_size": 16,
    "predict_batch_size": 4,
    "precision": "bf16",
    "dataset": "sordonia/flan-10k-flat",
    "model_family": "gpt",
    "load_in_8bit": false,
