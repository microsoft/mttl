{
    "model": "google/t5-large-lm-adapt",
    "dataset": "ni",
    "optimizer": "adamw",
    "data_dir": "$NI_DATA_DIR",
    "warmup_proportion": 0.06,
    "total_steps": 1000,
    "learning_rate": 1e-3,
    "max_grad_norm": 0.1,
    "weight_decay": 0.01,
    "train_batch_size": 8,
    "predict_batch_size": 8,
    "precision": "bf16",
    "use_task_descriptions": true,
    "num_pos_examples": 0,
    "max_num_instances_per_task": 32
}
