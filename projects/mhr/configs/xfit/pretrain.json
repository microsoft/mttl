{
    "dataset": "xfit",
    "optimizer": "adamw",
    "train_dir": "$XFIT_DATA_DIR",
    "custom_tasks_splits": "./mttl/dataloader/xfit_data/random.json",
    "warmup_proportion": 0.06,
    "num_train_epochs": 30,
    "learning_rate": 1e-3,
    "max_grad_norm": 0.1,
    "weight_decay": 0.01,
    "train_batch_size": 8,
    "predict_batch_size": 24,
    "max_input_length": 512,
    "max_output_length": 64
}
